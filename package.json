    "@rushstack/eslint-patch": "1.1.2",
    "@types/chai": "4.3.0",
    "@types/lodash": "4.14.181",
    "@types/mocha": "9.1.0",
    "@types/node": "16.11.26",
    "@types/sinon": "10.0.11",
    "@types/uuid": "8.3.4",
    "acorn": "8.7.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "chai-datetime": "1.8.0",
    "cheerio": "1.0.0-rc.10",
    "cls-hooked": "4.2.2",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "delay": "5.0.0",
    "esbuild": "0.14.36",
    "eslint": "8.13.0",
    "eslint-plugin-jsdoc": "39.2.0",
    "eslint-plugin-mocha": "10.0.4",
    "expect-type": "0.12.0",
    "fast-glob": "3.2.11",
    "fs-jetpack": "4.3.1",
    "husky": "7.0.4",
    "ibm_db": "2.8.1",
    "lcov-result-merger": "3.1.0",
    "lint-staged": "12.3.7",
    "mariadb": "3.0.0",
    "markdownlint-cli": "0.31.1",
    "mocha": "9.2.2",
    "module-alias": "2.2.2",
    "mysql2": "2.3.3",
    "node-hook": "1.0.0",
    "nyc": "15.1.0",
    "odbc": "2.4.3",
    "p-map": "4.0.0",
    "p-props": "4.0.0",
    "p-settle": "4.1.1",
    "p-timeout": "4.1.0",
    "pg": "8.7.3",
    "pg-hstore": "2.3.4",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.2",
    "semantic-release-fail-on-major-bump": "1.0.0",
    "sinon": "13.0.1",
    "sinon-chai": "3.7.0",
    "snowflake-sdk": "1.6.8",
    "source-map-support": "0.5.21",
    "sqlite3": "npm:@vscode/sqlite3@5.0.8",
    "tedious": "14.4.0",
    "typedoc": "0.22.15",
    "typescript": "4.6.3"
  },
  "peerDependenciesMeta": {
    "pg": {
      "optional": true
    },
    "pg-hstore": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "ibm_db": {
      "optional": true
    },
    "odbc": {
      "optional": true
    },
    "snowflake-sdk": {
      "optional": true
    },
    "mariadb": {
      "optional": true
    },
    "sqlite3": {
      "optional": true
    },
    "tedious": {
      "optional": true
    }
