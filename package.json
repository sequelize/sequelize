{
  "name": "sequelize",
  "description": "Multi dialect ORM for Node.JS/io.js",
  "version": "3.30.4",
  "author": "Sascha Depold <sascha@depold.com>",
  "contributors": [
    {
      "name": "Sascha Depold",
