      "optional": true
    },
    "ibm_db": {
      "optional": true
    },
    "odbc": {
      "optional": true
    },
    "snowflake-sdk": {
      "optional": true
    },
    "mariadb": {
      "optional": true
    },
    "sqlite3": {
