{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@sequelize/core": ["../types/index.d.ts"],
      "@sequelize/core/lib/*": ["../types/*"],
      "@sequelize/core/*": ["../*"]
