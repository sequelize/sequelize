{
  "bitwise":false,
  "boss":true,
  "expr":true,
  "camelcase":false,
  "curly":false,
  "eqeqeq":true,
  "freeze":true,
  "immed":true,
  "indent":2,
  "latedef":"nofunc",
  "laxbreak":true,
  "laxcomma":true,
  "newcap":true,
  "noarg":true,
  "node":true,
  "strict":true,
  "trailing":true,
  "undef":true,
  "esnext":false,
  "sub":true,

  /* questionable */
  "shadow":true,
  "loopfunc":true,
  "evil":true,

  "predef": [
    "alert",
    "describe",
    "it",
    "before",
    "beforeEach",
    "after",
    "afterEach"
  ]
}
