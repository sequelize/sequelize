{
  "extends": [
    "config:base",
    ":maintainLockFilesWeekly",
    ":semanticCommitTypeAll(meta)",
    ":semanticCommitScopeDisabled"
  ],
  "semanticCommitType": "meta",
  "ignorePaths": ["dev/**/oldest/docker-compose.yml"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ]
}
